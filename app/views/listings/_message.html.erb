<%-
    map_index = 0;
    if @map_index && @map_index > 0
        map_index=@map_index+1
    elsif @mapmessages
        if map_index=@mapmessages.index(@blog_entry)
            map_index=map_index+1;
        end
    else
        map_index="";
    end;
%>
<tr id="tr<%= @blog_entry.id %>" class="message">

  <% unless @blog_entry.price.nil? then %>
      <%= render :partial => 'listings/good_message_by_user', :locals =>{:map_index => map_index} %>
  <% else %>
      <%= render :partial => 'listings/bad_message_by_user', :locals =>{:map_index => map_index} %>
  <% end %>


</tr>
<% last_id = @blog_entry.id  unless @blog_entry.nil? %>